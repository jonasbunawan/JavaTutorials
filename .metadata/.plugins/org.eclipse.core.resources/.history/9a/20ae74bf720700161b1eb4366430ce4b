
public class Characters {
	public static void main(String[]args){
		char letter = 'a';
		System.out.println(letter);
		
		char upperK = 75;//assigning value to char variable by specifying unicode of the letter
		System.out.println(upperK);
		
		int K = 75;//assigning number to K
		System.out.println((char)K);//interpret K variable to character (unicode to letter)
		
		char upK = 'K';
		System.out.println((int)upK)//defining unicode of K
	}
}
