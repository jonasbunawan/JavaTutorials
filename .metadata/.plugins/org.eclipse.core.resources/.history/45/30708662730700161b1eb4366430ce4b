
public class BasicMath {
	public static void main(String[]args){
		int a = 8;
		int b = 9;
		
		int sum = a+b;
		System.out.println(sum);
		
		int dif = a-b;
		System.out.println(dif);
		
		int product = a*b;
		System.out.println(product);
		
		int division = a/b;
		System.out.println(division);
	}
}
